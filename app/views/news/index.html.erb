<br>
<div class="span12">
  <h2>Legal News and Updates</h2>
</div>
<div class="span12">
  <hr>
</div>
<div class="span12" id="news">
  <ul>
    <% @news.each do |news| %>
    <li><%= link_to(news.title, news_path(news), :target => :blank) %></li>
    <% end %>
  </ul>
</div>
<div class="span12">
  <hr>
  <div class="pull-right">
  <%= will_paginate @news %>
  </div>
</div>